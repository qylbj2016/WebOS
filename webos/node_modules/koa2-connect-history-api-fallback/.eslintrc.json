{
  "root": true,
  "parserOptions": {
    "parser": "typescript-eslint-parser",
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": [
      "error",
      "always",
      {
        "null": "ignore"
      }
    ],
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-undef": "warn"
  }
}
